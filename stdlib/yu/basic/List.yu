import Fun => yu/basic/Fun
of const


data List : Ty -> Ty
of nil [A : Ty] : List A
of (::#List) [A : Ty] : A & List A -> List A


val (.ind#List) [A : Ty] :
  (xs : List A) &
  (P : List A -> Ty) &
  ([] -> P nil) &
  ((y : A) & (ys : List A) & P ys -> P (y :: ys)) ->
  P xs
let nil P p0 pi => p0
let (x :: xs) P p0 pi => pi x xs (xs .ind P p0 pi)


val (.rec#List) [A : Ty] [B : Ty] :
  (xs : List A) & ([] -> B) & ((y : A) & List A & B -> B) -> B
let xs p0 pi => xs .ind (const B) p0 pi
