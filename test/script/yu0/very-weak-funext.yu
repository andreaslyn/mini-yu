data Wrap[A : Ty] : A -> Ty
| wrap[A : Ty] : (a : A) -> Wrap(a)

val veryWeakFunext [A : Ty] [B : A -> Ty]
  : (f : (a : A) -> B(a)) & (g : (a : A) -> B(a)) &
      (((a : A) -> Wrap(f a)) = ((a' : A) -> Wrap((g @ id.Fun) a'))) -> f = g
