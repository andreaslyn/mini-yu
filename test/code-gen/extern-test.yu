import "yu/prelude/prelude.yu"


extern (!_\) : Bool ->> Bool


extern len : (A : Ty) & List A -> Nat


val main : {} ->> {}
let () =>
  match (!_\) true
  let false => '1 .println
  let true => '0 .println
  end;
  match len Nat (0 :: 0 :: 0 :: nil)
  let 0 => "0\n" .print
  let _ => "1\n" .print
  end
